            {% set msg_sty = msg[0] %}
            {% set msg_desc = msg[1] %}
            var flashMsg = '{{msg_desc}}';
            var flashMsgType = '{{msg_sty}}';
            if ('danger' === flashMsgType) {
                toastr.error(flashMsg);
            }
            else if ('success' === flashMsgType) {
                toastr.success(flashMsg);
            } else {
                toastr.info(flashMsg);
            }
        {% endif %}
    });
